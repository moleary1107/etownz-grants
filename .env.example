# API Keys
OPENAI_API_KEY=your_openai_api_key_here
FIRECRAWL_API_KEY=your_firecrawl_api_key_here
PINECONE_API_KEY=your_pinecone_api_key_here

# Authentication
JWT_SECRET=your_jwt_secret_here
AUTH0_DOMAIN=your_auth0_domain
AUTH0_CLIENT_ID=your_auth0_client_id
AUTH0_CLIENT_SECRET=your_auth0_client_secret

# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/etownz_grants
REDIS_URL=redis://localhost:6379

# External Services
SENDGRID_API_KEY=your_sendgrid_api_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Digital Ocean
DO_SPACES_KEY=your_do_spaces_key
DO_SPACES_SECRET=your_do_spaces_secret
DO_SPACES_ENDPOINT=fra1.digitaloceanspaces.com
DO_SPACES_BUCKET=etownz-grants

# Environment
NODE_ENV=development
PORT=8000

# Message Queue
RABBITMQ_URL=amqp://guest:guest@localhost:5672

# Monitoring
SENTRY_DSN=your_sentry_dsn

# Crawling Configuration
FIRECRAWL_RATE_LIMIT=10
CRAWL_ENABLED=true
CRAWL_SCHEDULE="0 2 * * *"  # Daily at 2 AM

# DigitalOcean Deployment
DO_DROPLET_IP=your_droplet_ip_here
DO_SSH_KEY_PATH=~/.ssh/etownz_grants
DOMAIN_NAME=grants.etownz.com

# MCP Servers
MCP_DOCS_ENABLED=true
MCP_AUTOMATION_ENABLED=true